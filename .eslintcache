) {\r\n    switch(action.type) {\r\n        case AGREGAR_PRODUCTO:\r\n            return {\r\n                ...state,\r\n                loading: action.payload\r\n            }\r\n        case AGREGAR_PRODUCTO_EXITO:\r\n            return {\r\n                ...state,\r\n                loading: false,\r\n                productos: [...state.productos, action.payload]\r\n            }\r\n        case AGREGAR_PRODUCTO_ERROR:\r\n            return {\r\n                ...state,\r\n                loading: false,\r\n                error: action.payload\r\n            }\r\n\r\n        default:\r\n            return state;\r\n    }\r\n}","C:\\Users\\Usuario\\Desktop\\crudredux\\src\\types\\index.js",[],"C:\\Users\\Usuario\\Desktop\\crudredux\\src\\actions\\productoActions.js",[],"C:\\Users\\Usuario\\Desktop\\crudredux\\src\\config\\axios.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":2,"column":23,"nodeType":"67","messageId":"68","endLine":2,"endColumn":34},{"ruleId":"69","severity":1,"message":"70","line":15,"column":1,"nodeType":"71","endLine":38,"endColumn":2},"no-native-reassign",["72"],"no-negated-in-lhs",["73"],"no-unused-vars","'useSelector' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]